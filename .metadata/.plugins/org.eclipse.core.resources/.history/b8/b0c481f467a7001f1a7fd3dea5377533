package revamp;

public class patterns {

	public static void main(String[] args) {
		
		System.out.println("SHRI RADHE RADHE");
	/*  *****
		*****
		*****
		*****
		*****
		***** */
		
		int num = 5;
		
		for(int i = 0; i < num ; i++) {
			for(int j = 0; j < num ; j++) {
				System.out.print("*");
			}
			System.out.println();
		}
	}
}
